{
  "name": "21QuestionsWebApp",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "coveralls": "^2.11.12",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-react": "^6.0.0",
    "ignore-styles": "^4.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "node-sass": "^4.7.2",
    "react-addons-test-utils": "^15.3.0",
    "react-scripts": "0.2.1",
    "redux-mock-store": "^1.1.2",
    "sass-loader": "^6.0.6"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "enzyme": "^2.4.1",
    "expect": "latest",
    "highcharts": "^6.0.3",
    "isomorphic-fetch": "^2.2.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "pushstate-server": "latest",
    "react": "^15.6.2",
    "react-collapse": "^4.0.3",
    "react-dom": "^15.2.1",
    "react-highcharts": "^15.0.0",
    "react-motion": "^0.5.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "recharts": "^1.0.0-beta.6",
    "redux": "^3.5.2",
    "redux-saga": "^0.16.0",
    "socket.io-client": "^2.0.4"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "rm -rf build/ && react-scripts build",
    "build": "npm-run-all build-css build-js",
    "eject": "react-scripts eject",
    "test:coverage": "rm -rf coverage/ && istanbul cover  _mocha -- --compilers css:ignore-styles ./.setup.js \"test\"",
    "test": "clear && _mocha --compilers css:ignore-styles ./.setup.js \"test\" --recursive",
    "test:watch": "npm run test -- --watch",
    "deploy": "pushstate-server build",
    "homepage": "https://media-gallery.herokuapp.com",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
